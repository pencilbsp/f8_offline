<div>
  <h1 class="font-bold text-xl">Mail đã gửi</h1>

  <div class="overflow-x-auto">
    <table class="table">
      <!-- head -->
      <thead>
        <tr>
          <th></th>
          <th>Người nhận</th>
          <th>Tiêu đề</th>
          <th>Thời gian gửi</th>
          <th>Đã đọc lúc</th>
        </tr>
      </thead>
      <tbody>
        <!-- row 1 -->

        <% data.forEach(function(item, index) { %>
        <tr class="bg-base-200">
          <td><%= index + 1 %></td>
          <td><%= item.to %></td>
          <td><%= item.subject %></td>
          <td><%= item.created_at %></td>
          <td><%= item.seen_at %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</div>
